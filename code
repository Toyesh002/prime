#include <iostream>
using namespace std;

bool isPrime(int a)
{ 
    for(int i=2;i<a;i++){
        if(a%i==0){
            return 0;
            break;
        }
        else if(i==a-1){
            return 1;
        }
    }
}

int main(){
    int a;
    cin>>a;

if(a==1){
     cout<<"Number is neither prime nor co-prime";
}else
{
    int ans =isPrime(a);
    if(ans==0){
    cout<<"Number is not prime";
        }
    else{
    cout<<"Number is prime";
    }

}
}
